{
  "name": "korrekturleser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "run": "run-s build preview",
    "test": "vitest --watch=false",
    "test-cov": "vitest --watch=false --coverage",
    "cy:run": "cypress run --e2e",
    "cy:open": "cypress open --e2e",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --cache --fix --ignore-pattern .gitignore",
    "format": "prettier --cache --write vue_app/ cypress/ *.html *.ts *.json --log-level silent",
    "spell": "cspell --cache --quiet \"vue_app/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" \"docs/*\"",
    "spell-store": "cspell --cache --quiet --words-only --unique \"vue_app/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" > cspell-unknown-words.txt",
    "check": "run-p format lint type-check spell test",
    "generate-api": "openapi --input http://localhost:9002/openapi.json --output vue_app/src/api --client axios && uv run python scripts/gen_mode_descriptions.py"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "axios": "^1.13.2",
    "diff": "^8.0.3",
    "diff2html": "^3.4.55",
    "form-data": "^4.0.5",
    "marked": "^16.4.2",
    "pinia": "^3.0.4",
    "quasar": "^2.18.6",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "workbox-window": "^7.4.0"
  },
  "browserslist": [
    "iOS >= 15",
    "Safari >= 15",
    "> 0.5%",
    "not dead"
  ],
  "devDependencies": {
    "@cspell/dict-de-de": "^4.1.2",
    "@quasar/vite-plugin": "^1.10.0",
    "@tsconfig/node22": "^22.0.5",
    "@tsconfig/node24": "^24.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.9",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "baseline-browser-mapping": "^2.9.17",
    "cspell": "^9.6.0",
    "cypress": "^15.9.0",
    "eslint": "^9.39.2",
    "eslint-plugin-cypress": "^5.2.1",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "~10.5.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.8.1",
    "sass-embedded": "^1.97.3",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.3",
    "unplugin-vue-components": "^29.2.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.3"
  }
}
