# Database Configuration (only required for production)
# In local development, mock mode is used automatically
DB_DATABASE=xxx
DB_HOST=xxx
DB_PASS=xxx
DB_USER=xxx

# Gemini AI API Key
# Get your API key from https://aistudio.google.com/apikey
GEMINI_API_KEY=xxx

# JWT Secret for FastAPI
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=xxx

# Sentry DSN (optional - for error tracking)
SENTRY_DSN=https://XXX.ingest.de.sentry.io/YYYY

# API Base URL for FastAPI backend
VITE_API_BASE_URL=http://127.0.0.1:8504
